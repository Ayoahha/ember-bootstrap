<div role="tab" {{action "toggleActive"}} class="panel-heading {{if collapsed "collapsed"}}">
    <h4 class="panel-title">
        <a href="#">
            {{title}}
        </a>
    </h4>
</div>
<div class="panel-collapse {{if collapse "collapse"}} {{if in "in"}} {{if collapsing "collapsing"}}" style={{collapseStyle}} role="tabpanel">
    <div class="panel-body">
        {{yield}}
    </div>
</div>

