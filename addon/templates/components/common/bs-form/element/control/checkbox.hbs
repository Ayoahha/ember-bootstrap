<input
  type="checkbox"
  id={{@id}}
  disabled={{this.disabled}}
  readonly={{this.readonly}}
  aria-describedby={{this.ariaDescribedBy}}
  checked={{this.value}}
  class="{{if (has-bootstrap-version 4) "form-check-input"}} {{if (has-bootstrap-version 4) this.formValidationClass}}"
  ...attributes
  {{on "click" this.handleClick}}
>

