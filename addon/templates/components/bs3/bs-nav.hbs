<ul class="nav {{this.typeClass}} {{this.additionalClass}} {{if this.justified "nav-justified"}} {{if this.stacked "nav-stacked"}}" ...attributes>
  {{yield
    (hash
      item=(component this.itemComponent)
      link-to=(component this.linkToComponent)
      linkTo=(component this.linkToComponent)
      dropdown=(component this.dropdownComponent inNav=true htmlTag="li")
    )
  }}

</ul>